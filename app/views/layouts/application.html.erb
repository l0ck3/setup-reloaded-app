<!DOCTYPE html>
<html>
  <head>
    <title>SetupReloadedApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/asciinema-player/2.4.1/asciinema-player.js' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <div class="container">
      <%= yield %>
    </div>

    <%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/asciinema-player/2.4.1/asciinema-player.js' %>
  </body>
</html>
